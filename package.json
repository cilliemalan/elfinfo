    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@types/chai": "^4.2.11",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.12",
    "@types/supertest": "^2.0.9",
    "@wessberg/rollup-plugin-ts": "^1.2.24",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "jest": "^26.0.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "rollup": "^2.15.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
