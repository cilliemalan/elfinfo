  },
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "build": "tsc",
    "test": "jest --detectOpenHandles",
    "coverage": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "rollup": "rollup -c",
    "prepare": "npm run rollup && tsc && npm run gendocs",
    "gendocs": "jsdoc2md --files src/*.ts --template README.template.md --configure ./jsdoc2md.json  > README.md",
    "prepublishOnly": "jest --detectOpenHandles && tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
